  "targets": [
    {
      "datasource": {
        "type": "grafana-clickhouse-datasource",
        "uid": "ClickHouse"
      },
      "format": 1,
      "queryType": "sql",
      "rawSql": "SELECT \n    poi_name,\n    COUNT(*) as visits,\n    toStartOfHour(event_time) as time\nFROM nearyou.user_events\nWHERE event_time >= now() - INTERVAL 24 HOUR\nAND poi_range <= 500\nAND (poi_name IN (${selected_shops:sqlquote}) OR '$__all' IN (${selected_shops:sqlquote}))\nGROUP BY poi_name, time\nORDER BY visits DESC, time",
      "refId": "A"
    }
  ],
  "title": "Visite per Negozio (Ultimi 24 Ore)",
  "type": "timeseries"
