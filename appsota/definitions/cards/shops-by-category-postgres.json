{
    "name": "Shops per Categoria",
    "dataset_query": {
      "database": "near_you_shops",
      "type": "native",
      "native": {
        "query": "SELECT category, COUNT(*) AS cnt FROM shops GROUP BY category ORDER BY cnt DESC"
      }
    },
    "display": "bar",
    "visualization_settings": {
      "x_axis_field": "category",
      "y_axis_field": "cnt"
    }
  }
  