// dashboard-admin/frontend/tsconfig.json
{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["DOM", "ESNext"],
    "jsx": "react-jsx",                    
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,                  
    "forceConsistentCasingInFileNames": true,
    "strict": true
  },
  "include": ["src"]
}
